{
    "name": "Simple Toggle Sites",
    "version": "1.0.0",
    "description": "Simple Toggle Sites",
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "http://*/",
        "https://*/"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}
